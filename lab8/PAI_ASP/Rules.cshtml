@{
    var rules = new List<string>();

    if (Session["rules"] != null)
    {
        rules = (List<string>)Session["rules"];
    }
}

<!DOCTYPE html>
<html>
<head>
    <title></title>
</head>
<body>
    <form action="RulesRedirect.cshtml" method="POST">
        Treść reguły: <input type="text" name="FC" />
        <input type="submit" name="Dodaj" />
    </form>
    <ol>
        @for(int i=0;i< rules.Count();i++)
            {
                <li>@rules[i]</li>
            }
</ol>
    </body>
</html>
